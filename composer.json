{
    "name": "aisha-telegram-bot",
    "description": "AISHA - AI Girlfriend Telegram Bot",
    "type": "project",
    "require": {
        "php": "^8.1",
        "irazasyed/telegram-bot-sdk": "^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*",
        "ext-curl": "*",
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
